FUNKCIJA:GiveWeapons( playerid )
{
    LoadPlayerKomp( playerid );
    if ( !IsPlayerConnected( playerid ) ) return 1;
    for ( new gun = 0; gun < MAX_SAVED_WEAPONS; gun++ )
    {
        if ( pInfo[ playerid ][ pGun ][ gun ] > 0 && pInfo[ playerid ][ pAmmo ][ gun ] > 0 )
