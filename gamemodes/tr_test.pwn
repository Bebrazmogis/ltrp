        printf( "Tr. priemonës MySQL ID: %d (%s), sunaikinusiojo ID: %d (%s)", cInfo[ vehicleid ][ cID ], GetVehicleName( GetVehicleModel( vehicleid ) ), killerid, GetName( killerid ));
