                if(Kils[GetPlayerVehicleID ( i )] > 2)
                {
                    Kils[ GetPlayerVehicleID ( i ) ] = 0;
                    cInfo[ GetPlayerVehicleID ( i ) ][ cKM ] += 1.0;
                }
            }
        }
        if(Mires[i] > 1)
