    LaikoTipas[ playerid ] = 0;

    if ( Checkpoint[ playerid ] != CHECKPOINT_NONE )
    {
        DisablePlayerCheckpoint( playerid );
        Checkpoint[ playerid ] = CHECKPOINT_NONE;
    }
    if(CheckUnfreeze(playerid))
        TogglePlayerControllable(playerid,true);
    

    return 1;
}
