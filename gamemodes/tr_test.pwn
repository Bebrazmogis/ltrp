stock ClearWeaponsFromInv( playerid )
{
    for ( new i = 0; i < INVENTORY_SLOTS; i++ )
    {
        if ( InvInfo[ playerid ][ i ][ iID ] < 50 )
        {
            InvInfo[ playerid ][ i ][ iID      ] = 0;
            InvInfo[ playerid ][ i ][ iAmmount ] = 0;
        }
    }
    return 1;
}
