    if ( pInfo[ playerid ][ pTester ] >= 1 || pInfo[ playerid ][ pAdmin ] >= 1 )
    {
        format( string, 256, "[Mod] %s: %s ", GetName( playerid ), string );
        foreach(Player,i)
        {
            if ( pInfo[ i ][ pTester ] >= 1 && GetPVarInt( i, "TESTER_TOG" ) == 0 )
