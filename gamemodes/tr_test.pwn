        timeris[i]++;
        
        if( GetPlayerState(i) == 2 && Engine[GetPlayerVehicleID ( i )] == true && VehicleHasEngine( GetVehicleModel( GetPlayerVehicleID ( i ) ) ) && timeris[i] > 2 )
