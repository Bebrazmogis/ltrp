    if ( fInfo[ PlayerFaction( playerid ) ][ fChat ] == 1 && pInfo[ playerid ][ pLead ] == 0)
        return SendClientMessage( playerid, COLOR_LIGHTRED, "Klaida, frakcijos kanalas (/f) ðiuo metu yra iðjungtas. Susisiekite su frakcijos vadovu." );

    new
