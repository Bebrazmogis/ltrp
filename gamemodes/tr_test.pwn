            PutPlayerInVehicle(playerid, NPCTrain, 0);
        return 1;
    }
    
    if ( PlayerOn[ playerid ] == false )
    {
        SendClientMessage( playerid, GRAD, "Jûs nesate prisijungæs prie serverio, praðome prisijungti." );
        Kick( playerid );
        return 1;
    }
    ObjUpdate[ playerid ] = true;

