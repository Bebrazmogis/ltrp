stock GetVehicleOwnerName( id )
{
    new name[MAX_PLAYER_NAME+1],
        string[ 256 ];
    
    format(string, sizeof(string), "SELECT `Name` FROM `players` WHERE `id` = %d", id );
    mysql_query(string);
    mysql_store_result();
    mysql_fetch_row(name);
