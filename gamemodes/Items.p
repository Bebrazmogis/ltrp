	for(new i = 0; i < sizeof(ItemData); i++)
	{
		printf("GetItemId(%s). ItemData[%d][Name]:%s ItemData[i][Id]:%d",
			itemname, i, ItemData[ i ][ Name ], ItemData[ i ][ Id ]);
